{
        "name": "dsfml",
        "description": "DSFML binding for SFML-The Simple and Fast Multimedia Library",
        "authors": ["Jeremy DeHaan"],
        "homepage": "https://github.com/Jebbs/DSFML",
        "version":"2.5.0",
        "license": "Zlib",
        "lflags": ["-L/usr/local/lib/"],
        "targetPath": "lib",
        "dependencies": {
            "dsfml:audio": "~>2.5",
            "dsfml:graphics": "~>2.5",
            "dsfml:window": "~>2.5",
            "dsfml:network": "~>2.5",
            "dsfml:system": "~>2.5"
        },
        "subPackages":
        [
                {
                        "name": "audio",
                        "targetType": "library",
                        "targetPath": "lib",
                        "sourcePaths": ["src/dsfml/audio"],
                        "libs": ["csfml-audio"],
                        "dependencies":
                        {
                                "dsfml:system": "~>2.5"
                        }
                },
                {
                        "name": "graphics",
                        "targetType": "library",
                        "targetPath": "lib",
                        "sourcePaths": ["src/dsfml/graphics"],
                        "libs": ["csfml-graphics"],
                        "dependencies":
                        {
                                "dsfml:system": "~>2.5",
                                "dsfml:window": "~>2.5"
                        }
                },
                {
                        "name": "window",
                        "targetType": "library",
                        "targetPath": "lib",
                        "sourcePaths": ["src/dsfml/window"],
                        "libs": ["csfml-window"],
                        "dependencies":
                        {
                                "dsfml:system": "~>2.5"
                        }
                },
                {
                        "name": "network",
                        "targetType": "library",
                        "targetPath": "lib",
                        "sourcePaths": ["src/dsfml/network"],
                        "libs": ["csfml-network"],
                        "dependencies":
                        {
                                "dsfml:system": "~>2.5"
                        }
                },
                {
                        "name": "system",
                        "targetType": "library",
                        "targetPath": "lib",
                        "importPaths": ["src/"],
                        "sourcePaths": ["src/dsfml/system"],
                        "libs": ["csfml-system"]
                }
        ]
}
